<serialization jsxversion="3.6" xmlns="urn:tibco.com/v3.0">
  <name><![CDATA[Select - Combo]]></name>
  <icon><![CDATA[images/prototypes/combo.gif]]></icon>
  <description><![CDATA[Combo/Select field.  Allows the user to enter text manually or move through the list of options with typeahead.]]></description>
  <onBeforeDeserialize></onBeforeDeserialize>
  <onAfterDeserialize></onAfterDeserialize>
  <object type="com.intalio.ria.Field">
    <variants/>
    <strings jsxname="combo-field"
        jsxclassname="field"
        riaLabelText="[Combo label]"
        riaHelpImageSrc="images/help.gif" 
        riaErrorImageSrc="images/error.gif">
    </strings>
    <object type="jsx3.gui.Select">
      <variants jsxwidth="150" jsxheight="18" jsxtype="1" jsxxmlasync="1"/>
      <strings jsxname="combo" jsxmargin="0 4 0 0" jsxxml="
          &lt;data jsxid=&quot;jsxroot&quot;&gt; 
            &lt;record jsxid=&quot;Automobile&quot; jsxtext=&quot;Automobile&quot;/&gt; 
            &lt;record jsxid=&quot;Boat&quot; jsxtext=&quot;Boat&quot;/&gt; 
            &lt;record jsxid=&quot;Bus&quot; jsxtext=&quot;Bus&quot;/&gt; 
            &lt;record jsxid=&quot;Helicopter&quot; jsxtext=&quot;Helicopter&quot;/&gt; 
            &lt;record jsxid=&quot;Motorcycle&quot; jsxtext=&quot;Motorcycle&quot;/&gt; 
            &lt;record jsxid=&quot;Plane&quot; jsxtext=&quot;Plane&quot;/&gt; 
            &lt;record jsxid=&quot;Spaceship&quot; jsxtext=&quot;Spaceship&quot;/&gt; 
            &lt;record jsxid=&quot;Train&quot; jsxtext=&quot;Train&quot;/&gt; 
          &lt;/data&gt;
          "/>
      <events jsxselect="com.intalio.ria.validate(this, objEVENT);"/>
    </object>
  </object>
</serialization>
