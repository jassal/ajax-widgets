<serialization jsxversion="3.6" xmlns="urn:tibco.com/v3.0">
  <name><![CDATA[Slider]]></name>
  <icon><![CDATA[images/prototypes/slider.gif]]></icon>
  <description><![CDATA[Slider field.]]></description>
  <onBeforeDeserialize></onBeforeDeserialize>
  <onAfterDeserialize></onAfterDeserialize>
  <object type="com.intalio.ria.Field">
    <variants/>
    <strings jsxname="slider-field"
        riaLabelText="[Slider label]"
        riaHelpImageSrc="images/help.gif" 
        riaErrorImageSrc="images/error.gif"
        riaCssClass="field">
    </strings>
    <object type="jsx3.gui.Slider">
      <variants jsxvalue="0" jsxrelativeposition="1" jsxorientation="0"/>
      <strings jsxname="slider" jsxlength="200"/>
      <events jsxchange="com.intalio.ria.setValue(this, fpVALUE);" 
              jsxincchange="com.intalio.ria.setValue(this, fpVALUE);"/>      
    </object>
  </object>  
</serialization>

