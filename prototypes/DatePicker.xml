<serialization jsxversion="3.6" xmlns="urn:tibco.com/v3.0">
  <name><![CDATA[Date Picker]]></name>
  <icon><![CDATA[images/prototypes/datepicker.gif]]></icon>
  <description><![CDATA[Date picker field.]]></description>
  <onBeforeDeserialize></onBeforeDeserialize>
  <onAfterDeserialize></onAfterDeserialize>
  <object type="com.intalio.ria.Field">
    <variants/>
    <strings jsxname="datepicker-field"
        riaLabelText="[Data picker label]"
        riaHelpImageSrc="images/help.gif" 
        riaErrorImageSrc="images/error.gif"
        riaCssClass="field">
    </strings>
    <object type="jsx3.gui.DatePicker">
      <variants jsxwidth="100" jsxrelativeposition="1" jsxheight="18"/>
      <strings jsxname="datePicker" jsxmargin="0 4 0 0" jsxformat="yyyy-MM-dd"/>
      <events jsxhide="com.intalio.ria.validate(this);"/>      
    </object>
  </object>
</serialization>

